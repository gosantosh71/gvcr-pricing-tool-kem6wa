{
  "dashboard": {
    "id": "business-dashboard",
    "uid": "business-dashboard",
    "title": "VAT Filing Pricing Tool - Business Dashboard",
    "tags": ["business", "kpi", "metrics", "vatfiling"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1,
    "editable": false
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\", severity=~\"critical|warning\", category=\"business\"}",
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Business Alerts",
        "titleFormat": "Alert: {{alertname}}",
        "textFormat": "{{description}}"
      }
    ]
  },
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "timeRange",
        "type": "interval",
        "query": "1h,6h,12h,24h,7d,30d,90d",
        "current": {
          "selected": true,
          "text": "24h",
          "value": "24h"
        }
      },
      {
        "name": "country",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(vatfilingpricingtool_calculations_by_country_total, country)",
        "multi": true,
        "includeAll": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": ["$__all"]
        }
      },
      {
        "name": "service_type",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(vatfilingpricingtool_calculations_by_service_type_total, service_type)",
        "multi": true,
        "includeAll": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": ["$__all"]
        }
      }
    ]
  },
  "panels": [
    {
      "title": "Business Overview",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "title": "Active Users",
      "type": "stat",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(vatfilingpricingtool_user_activity_total[$timeRange]))",
          "legendFormat": "Active Users"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 50
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "green",
                "value": 200
              }
