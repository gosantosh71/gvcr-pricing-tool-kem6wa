      "type": "table",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "ALERTS{alertstate=\"firing\", category=\"business\"}",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "severity",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "severity"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "critical": {
                        "color": "red",
                        "index": 0
                      },
                      "warning": {
                        "color": "orange",
                        "index": 1
                      },
                      "info": {
                        "color": "blue",
                        "index": 2
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
